{
  "functions": {
    "app/(apps)/api/incognito/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/incognito/**"
    },
    "app/(apps)/api/voice/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/voice/**"
    },
    "app/(apps)/api/transcribe/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/transcribe/**"
    },
    "app/(apps)/api/paper-flashcards/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/paper-flashcards/**"
    },
    "app/(apps)/api/filechat/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/filechat/**"
    },
    "app/(apps)/api/imagen/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/imagen/**"
    },
    "app/(apps)/api/search/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/search/**"
    },
    "app/(apps)/api/web/route.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "app/(apps)/api/web/**"
    }
  },
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm build",
  "outputDirectory": ".next"
}
